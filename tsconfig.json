{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@blocks/*": ["./src/blocks/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@images/*": ["./src/images/*"],
      "@style/*": ["./src/style/*"]
    }
  }
}
