{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*", "src", "**/*.astro"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@images/*": ["./src/images/*"],
      "@styles/*": ["./src/styles/*"],
      "@base/*": ["./*"]
    }

  }
}
