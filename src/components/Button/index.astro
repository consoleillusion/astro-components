---
import {loadPropDefaults} from "../../loadPropDefaults.js"
import "./style.scss"
const props = await loadPropDefaults(import.meta.dirname + '/config.yaml',Astro.props)
const actionType = "ahref"
---

{ (actionType === "ahref")
? (<a href={props.action} role="button" class={props.class}>{props.text}</a>)
: (<button {props.action} class={props.class} >{props.text}</button>)
}
